  object qryGrabarE: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      
        'insert into Equipos (ficha, marca, modelo, chasis, registro, seg' +
        'uro,'
      'color, capacidad, placa, porciento, user_e, sta)'
      
        'values (:vfichae, :vmarca, :vmodelo, :vchasis, :vreg, :vseguro, ' +
        ':vcolor,'
      ':vcap, :vplaca, :vporc, :vusere, :vstae) ')
    FetchAll = True
    Left = 96
    Top = 45
    ParamData = <
      item
        DataType = ftString
        Name = 'vfichae'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'vmarca'
      end
      item
        DataType = ftUnknown
        Name = 'vmodelo'
      end
      item
        DataType = ftUnknown
        Name = 'vchasis'
      end
      item
        DataType = ftUnknown
        Name = 'vreg'
      end
      item
        DataType = ftUnknown
        Name = 'vseguro'
      end
      item
        DataType = ftUnknown
        Name = 'vcolor'
      end
      item
        DataType = ftUnknown
        Name = 'vcap'
      end
      item
        DataType = ftUnknown
        Name = 'vplaca'
      end
      item
        DataType = ftUnknown
        Name = 'vporc'
      end
      item
        DataType = ftUnknown
        Name = 'vusere'
      end
      item
        DataType = ftUnknown
        Name = 'vstae'
      end>
  end
  object qryBorrarE: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      'Delete from Equipos'
      'where ficha = :bficha')
    FetchAll = True
    Left = 168
    Top = 48
    ParamData = <
      item
        DataType = ftString
        Name = 'bficha'
        Value = ''
      end>
  end
  object qryModificarE: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      'update Equipos'
      'set ficha=:vfichae, marca=:vmarca, modelo=:vmodelo, '
      
        'chasis=:vchasis, registro=:vreg, seguro=:vseguro, color=:vcolor,' +
        ' '
      
        'capacidad=:vcap, placa=:vplaca, porciento=:vporc, user_e=:vusere' +
        ','
      'sta=:vstae'
      'where ficha =:v1ficha')
    FetchAll = True
    Left = 240
    Top = 45
    ParamData = <
      item
        DataType = ftString
        Name = 'vfichae'
        Value = ''
      end
      item
        DataType = ftUnknown
        Name = 'vmarca'
      end
      item
        DataType = ftUnknown
        Name = 'vmodelo'
      end
      item
        DataType = ftUnknown
        Name = 'vchasis'
      end
      item
        DataType = ftUnknown
        Name = 'vreg'
      end
      item
        DataType = ftUnknown
        Name = 'vseguro'
      end
      item
        DataType = ftUnknown
        Name = 'vcolor'
      end
      item
        DataType = ftUnknown
        Name = 'vcap'
      end
      item
        DataType = ftUnknown
        Name = 'vplaca'
      end
      item
        DataType = ftUnknown
        Name = 'vporc'
      end
      item
        DataType = ftUnknown
        Name = 'vusere'
      end
      item
        DataType = ftUnknown
        Name = 'vstae'
      end
      item
        DataType = ftUnknown
        Name = 'v1ficha'
      end>
  end
  object qryConsultaE: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      'select * from Equipos'
      'order by ficha')
    FetchAll = True
    Left = 320
    Top = 45
