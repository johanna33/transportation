  object qryModifi: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      'update Inventario'
      'set codi=:vcodi, fechai=:vfechai, articulo=:varti, nota=:vnota,'
      'precio=:vprecio, canti=:vcanti, user_i=:vuseri'
      'where codi=:v1codi')
    FetchAll = True
    Left = 240
    Top = 49
    ParamData = <
