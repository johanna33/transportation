  object qryGrabari: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      
        'insert into Inventario (codi, fechai, articulo, precio, canti, u' +
        'ser_i, nota)'
      
        'values (:vcodi, :vfechai, :varti, :vprecio, :vcanti, :vuseri, :v' +
        'nota) ')
    FetchAll = True
    Left = 104
    Top = 49
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'vcodi'
      end
      item
        DataType = ftUnknown
        Name = 'vfechai'
