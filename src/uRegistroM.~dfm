  object qryGrabarM: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      
        'insert into Mantenimiento (cod_m, fecham, recibo, descrip, nombr' +
        'em, ficham, '
      'marcam, kmt, importe, user_m)'
      
        'values (:vcodm, :vfecham, :vrecibo, :vdescrip, :vnombrem, :vfich' +
        'am, :vmarcam,'
      ':vkmt, :vimporte, :vuserm) ')
    Left = 88
    Top = 45
    ParamData = <
      item
        DataType = ftUnknown
