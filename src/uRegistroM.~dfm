  object qryModificarM: TMyQuery
    Connection = fPrincipal.MyConnectT
    SQL.Strings = (
      'update Mantenimiento'
      'set cod_m=:vcodm, fecham=:vfecham, recibo=:vrecibo, '
      'descrip=:vdescrip, nombrem=:vnombrem, ficham=:vficham,'
      'marcam=:vmarcam, kmt=:vkmt, importe=:vimporte, user_m=:vuserm'
      'where cod_m=:v1mant')
    Left = 232
    Top = 45
    ParamData = <
